<h1>Welcome to the Movie Library!!</h1>

<table>
<thead>
    <tr>
        <h3>Title </h3>
    </tr>
    <br />
</thead>
<tbody>
    <% @libraries.each do |library| %>
        <ul>
            <li> <%= library.title %> </li>
            <%= link_to 'See Movies', library_path(library), class: 'btn' %>
            <%= link_to 'Delete Library', library, method: :delete %>
        </ul>
    <% end %>
    <%= link_to 'Add Movies', library_movies_path(:library_id)%>
</tbody>
</table>