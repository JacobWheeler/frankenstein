<h2>Welcome <%= current_user.first_name %> </h2>

<% if current_user.movies.any?%>
    <%current_user.movies do |movie|%>
    <ul>
        <li>
            <%= movie.title %>
            <%=link_to 'Movie Details', movie_path%>
        </li>
    </ul>
<% end %>
<% else %>
    <h3> Please Create a Library </h3>
<% end %>




<%= link_to 'Create Library', new_library_path%>
<%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
